<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>SUPABASE_URL</key>
    <string>your-supabase-url</string>
    <key>SUPABASE_ANON_KEY</key>
    <string>your-supabase-anon-key</string>
    <key>REVENUE_CAT_API_KEY</key>
    <string>your-revenue-cat-api-key</string>
    <key>CLAUDE_API_KEY</key>
    <string>your-claude-api-key</string>
    <!-- All other API keys (CoinGecko, Metals, TAAPI, Coinglass, FRED, Finnhub) -->
    <!-- are stored as Supabase Edge Function secrets and injected server-side. -->
    <!-- Set them with: supabase secrets set KEY=value -->
</dict>
</plist>
